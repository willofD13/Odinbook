<h1> Users </h1>
<% @users.each do |u| %>
    <% if u != current_user %>
        <ul>
        <li><%= u.name %></li>
        </ul>
        <% a =[] %>
        <% current_user.given_follows.each do |f|  %>
            <% a << f.followed_user_id %></li>
        <% end %>
        <% if !a.include?(u.id) %>
            <ul>
            <%= render "follows/form", user: u %>
            </ul>
        <% end %>
    <% end %>
<% end %>