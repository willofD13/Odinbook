<h1> Posts </h1>

<%= turbo_frame_tag id="new_post", src:new_post_path %>
<div id="posts">
  <%= render @posts %>
</div>




<h3> Users you follow: </h3>
<% current_user.given_follows.each do |f|  %>
    <ul>
    <li><%= f.followed_user.name %></li>
    </ul>
<% end %>

<%= link_to 'See users', users_path %>
<%= button_to 'Sign out', destroy_user_session_path, method: :delete %>
<%= link_to 'Edit account', edit_user_registration_path %>
